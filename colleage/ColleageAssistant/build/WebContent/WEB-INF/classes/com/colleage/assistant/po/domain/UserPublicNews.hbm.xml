<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.colleage.assistant.po.domain">
	<class name="UserPublicNews" table="user_public_news">
		<!-- 映射标识属性 -->
		<id name="id" column="id">
		<!-- 指定主键s生成策略 -->
		<generator class="identity" />
		</id>
		<!-- 映射普通属性 -->
		<property name="title" column="title" not-null="true"></property>
		<property name="content" column="content" not-null="true"></property>
		<property name="news_kind_id" column="news_kind_id"></property>
		<property name="picture1" column="picture1" ></property>
		<property name="picture2" column="picture2" ></property>
		<property name="picture3" column="picture3" ></property>
		<property name="public_time" column="public_time" ></property>
		</class>
</hibernate-mapping>