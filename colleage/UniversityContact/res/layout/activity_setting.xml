<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    android:orientation="vertical"
     >

	<include 
	    android:id="@+id/title_register"
	    layout="@layout/title_bar"
	    />

	<RelativeLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:padding="12dp"
	    >
	    
	    <RelativeLayout 
	    android:id="@+id/clear_stroe"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:background="@drawable/shape_all_corner"
	     android:clickable="true"
	    >
	     <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="5dp"
	            android:layout_centerVertical="true"
	            android:text="清除缓存"
	            android:textColor="@color/black"
	            android:textSize="20sp"
               />
	     <TextView 
	         android:id="@+id/clear_size"
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_toLeftOf="@+id/row_img_01"
	            android:layout_centerVertical="true"
	            android:text="20M"
	            android:textColor="@color/red"
	            android:textSize="20sp"
               />
	     	<ImageView 
               	    android:id="@+id/row_img_01"
            	android:layout_width="wrap_content"
        		android:layout_height="wrap_content" 
        		android:layout_centerVertical="true"
        		android:layout_alignParentRight="true"
        		android:layout_marginRight="10dip"      	     	
        		android:scaleType="matrix"
        		android:src="@drawable/login_nextpage"
               	    />
	    
	</RelativeLayout>
	    <RelativeLayout 
	    android:id="@+id/nigte_model"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:layout_below="@+id/clear_stroe"
	    android:layout_marginTop="10dp"
	    android:background="@drawable/shape_all_corner"
	    >
	     <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="5dp"
	            android:layout_centerVertical="true"
	            android:text="夜间模式"
	            android:textColor="@color/black"
	            android:textSize="18sp"
               />
	     	<com.wtu.university.tools.SlipButton 
	     	    android:id="@+id/save_pic"
            	 android:layout_width="80dip"
                 android:layout_height="25dip"
        		android:layout_centerVertical="true"
        		android:layout_alignParentRight="true"
        		android:layout_marginRight="-15dip"    	     	
               	    />
	    
	</RelativeLayout>
	    <RelativeLayout 
	    android:id="@+id/tuisong_sing"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	     android:layout_below="@+id/nigte_model"
	    android:layout_marginTop="10dp"
	    android:background="@drawable/shape_all_corner"
	    >
	     <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="5dp"
	            android:layout_centerVertical="true"
	            android:text="推送铃声"
	            android:textColor="@color/black"
	            android:textSize="18sp"
               />
	     	<com.wtu.university.tools.SlipButton 
	     	    android:id="@+id/save_pic_ping"
            	 android:layout_width="80dip"
                 android:layout_height="25dip"
        		android:layout_centerVertical="true"
        		android:layout_alignParentRight="true"
        		android:layout_marginRight="-15dip"     	     	
               	    />
	    
	</RelativeLayout>

	    <RelativeLayout
	        android:id="@+id/recieve_tuisong"
	        android:layout_width="match_parent"
	        android:layout_height="50dp"
	        android:layout_below="@+id/tuisong_sing"
	        android:background="@drawable/shape_all_corner"
	        android:gravity="center_vertical" >

	     <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="5dp"
	            android:layout_centerVertical="true"
	            android:text="接收推送热点"
	            android:textColor="@color/black"
	            android:textSize="18sp"
               />
	     	<com.wtu.university.tools.SlipButton 
	     	    android:id="@+id/recive_pic_ping"
            	 android:layout_width="80dip"
                 android:layout_height="25dip"
        		android:layout_centerVertical="true"
        		android:layout_alignParentRight="true"
        		android:layout_marginRight="-15dip"      	     	
               	    />
	    
	</RelativeLayout>
	    <RelativeLayout 
	    android:id="@+id/recieve_huifu"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	     android:layout_below="@+id/recieve_tuisong"
	    android:background="@drawable/shape_all_corner"
	    >
	     <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="5dp"
	            android:layout_centerVertical="true"
	            android:text="接收回帖提醒"
	            android:textColor="@color/black"
	            android:textSize="18sp"
               />
	     	<com.wtu.university.tools.SlipButton 
	     	    android:id="@+id/recive_pic_hufu"
            	 android:layout_width="80dip"
                 android:layout_height="25dip"
        		android:layout_centerVertical="true"
        		android:layout_alignParentRight="true"
        		android:layout_marginRight="-15dip"     	     	
               	    />
	    
	</RelativeLayout>
	    
	    <RelativeLayout 
	    android:id="@+id/help_sugest"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	     android:layout_below="@+id/recieve_huifu"
	    android:background="@drawable/shape_all_corner"
	     android:layout_marginTop="10dp"
	     android:clickable="true"
	    >
	     <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="5dp"
	            android:layout_centerVertical="true"
	            android:text="帮助反馈"
	            android:textColor="@color/black"
	            android:textSize="18sp"
               />
	     	<ImageView 
            	android:layout_width="wrap_content"
        		android:layout_height="wrap_content" 
        		android:layout_centerVertical="true"
        		android:layout_alignParentRight="true"
        		android:layout_marginRight="10dip"      	     	
        		android:scaleType="matrix"
        		android:src="@drawable/login_nextpage"
               	    />
	    
	</RelativeLayout>
	    <RelativeLayout 
	    android:id="@+id/aboutAPP"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	     android:layout_below="@+id/help_sugest"
	    android:background="@drawable/shape_all_corner"
	     android:clickable="true"
	    >
	     <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="5dp"
	            android:layout_centerVertical="true"
	            android:text="关于高校帮"
	            android:textColor="@color/black"
	            android:textSize="18sp"
               />
	     	<ImageView 
            	android:layout_width="wrap_content"
        		android:layout_height="wrap_content" 
        		android:layout_centerVertical="true"
        		android:layout_alignParentRight="true"
        		android:layout_marginRight="10dip"      	     	
        		android:scaleType="matrix"
        		android:src="@drawable/login_nextpage"
               	    />
	    
	</RelativeLayout>
	    <RelativeLayout 
	    android:id="@+id/suggest_to_friends"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	     android:layout_below="@+id/aboutAPP"
	    android:background="@drawable/shape_all_corner"
	     android:clickable="true"
	    >
	     <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="5dp"
	            android:layout_centerVertical="true"
	            android:text="推荐给好友"
	            android:textColor="@color/black"
	            android:textSize="18sp"
               />
	     	<ImageView 
            	android:layout_width="wrap_content"
        		android:layout_height="wrap_content" 
        		android:layout_centerVertical="true"
        		android:layout_alignParentRight="true"
        		android:layout_marginRight="10dip"      	     	
        		android:scaleType="matrix"
        		android:src="@drawable/login_nextpage"
               	    />
	    
	</RelativeLayout>
	    
	      <RelativeLayout
	          android:id="@+id/logout"
	          android:layout_width="match_parent"
	          android:layout_height="50dp"
	          android:layout_below="@+id/suggest_to_friends"
	          android:background="@drawable/login_click"
	          android:clickable="true"
	           android:layout_marginTop="10dp"
	          android:gravity="center_horizontal" >

	     	 <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:text="注销"
	            android:textColor="@color/white"
	            android:textSize="18sp"
               />
	    </RelativeLayout>
	      <RelativeLayout
	          android:id="@+id/exitAPP"
	          android:layout_width="match_parent"
	          android:layout_height="50dp"
	          android:layout_below="@+id/logout"
	          android:layout_marginTop="2dp"
	          android:background="@drawable/login_click"
	          android:clickable="true"
	          android:gravity="center_horizontal" >

	     	 <TextView 
                 android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:text="退出"
	            android:textColor="@color/white"
	            android:textSize="18sp"
               />
	    </RelativeLayout>
	</RelativeLayout>
	
</LinearLayout>
