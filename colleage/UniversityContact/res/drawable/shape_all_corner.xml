<?xml version="1.0" encoding="UTF-8"?>
<!-- 底部圆角 白色背景 灰色边框 长方体 -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <solid android:color="#FFFFFF" />

            <corners android:radius="10dp" />

            <stroke
                android:width="1dp"
                android:color="#ffa8abad" />

            <padding
                android:left="10dp"
                android:right="10dp" />
        </shape>
    </item>

</layer-list>